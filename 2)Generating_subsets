//generating all subsets of a set of n elements
#include "bits/stdc++.h"
using namespace std;

#define N 100005

int n, a[N];

bool vis[N];

void f(int x) {
    if (x == n + 1) {
        for (int i = 1; i <= n; i++){
            if (vis[i]) cout << a[i] << ' ';
        }
        cout << "\n";
        return;
    }
    for (int i = 0; i < 2; i++) {
        vis[x] = i;
        f(x + 1);
    }
}

int main() {
    ios::sync_with_stdio(false); cin.tie(nullptr);

    cin >> n;
    for (int i = 1; i <= n; i++) cin >> a[i];
    f(1);
}
